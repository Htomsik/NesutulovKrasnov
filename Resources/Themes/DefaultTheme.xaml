<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">



    <!--  Для фона  -->
    <SolidColorBrush x:Key="60Color" Color="#3C4DB9" />

    <!--  Для меню и больших кнопок  -->
    <SolidColorBrush x:Key="30Color" Color="#C8B156" />

    <!--  Текст  -->
    <SolidColorBrush x:Key="10Color" Color="#D6D3DA" />

    <!--  Дополнительный цвет  -->
    <SolidColorBrush x:Key="DopColor" Color="#1C1C22" />

    <!--  Цвет при наведении на кнопку  -->
    <SolidColorBrush x:Key="DopColorIsFocused" Color="#535365" />

    <SolidColorBrush x:Key="Menucolor" Color="#1C1C22" />

    <!--  ......................... раздел на случай где нельзя использовать солиды .........................  -->

    <Color x:Key="60Color(color)">#3C4DB9</Color>

    <Color x:Key="30Color(color)">#C8B156</Color>

    <Color x:Key="10Color(color)">#D2D3DA</Color>

    <Color x:Key="DopColor(color)">#1C1C22</Color>

    <Color x:Key="DopColorIsFocused(color)">#535365</Color>




</ResourceDictionary>