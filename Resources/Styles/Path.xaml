<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <!--  Для авторизации и регистрации  -->

    <Style x:Key="AuthRegPath" TargetType="{x:Type Path}">
        <Setter Property="Width" Value="280" />
        <Setter Property="Height" Value="1" />
        <Setter Property="Margin" Value="0,0,0,10" />
        <Setter Property="VerticalAlignment" Value="Bottom" />
        <Setter Property="Stretch" Value="Fill" />
        <Setter Property="Fill" Value="{DynamicResource 10Color}" />
        <Setter Property="Stroke" Value="{DynamicResource 10Color}" />
        <Setter Property="Data">
            <Setter.Value>
                <LineGeometry StartPoint="0,0" EndPoint="100,0" />
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="MainWindowPath" TargetType="{x:Type Path}">
        <Setter Property="Fill" Value="{DynamicResource 60Color}" />
        <Setter Property="Data" Value="M0,700 L0,700 0,0 1000,0 1000,350 C993.5,356.5 969.5,624.5 930.5,482.5 891.5,340.5 855.5,785.5 792.5,537.5 729.5,289.5 735.5,774.5 665.5,589.5 595.5,450 680,1000 0,700 z" />
    </Style>

</ResourceDictionary>