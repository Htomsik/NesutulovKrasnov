<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <!--  Для авторизации и регистрации  -->

    <Style x:Key="AuthRegPath" TargetType="{x:Type Path}">
        <Setter Property="Width" Value="280" />
        <Setter Property="Height" Value="1" />
        <Setter Property="Margin" Value="0,0,0,10" />
        <Setter Property="VerticalAlignment" Value="Bottom" />
        <Setter Property="Stretch" Value="Fill" />
        <Setter Property="Fill" Value="{DynamicResource 10Color}" />
        <Setter Property="Stroke" Value="{DynamicResource 10Color}" />
        <Setter Property="Data">
            <Setter.Value>
                <LineGeometry StartPoint="0,0" EndPoint="100,0" />
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>